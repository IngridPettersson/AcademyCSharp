@model WebAPI.Models.ViewModels.PhotosIndexVM[]


<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Photos</title>
</head>
<body>
    <h1>Photos</h1>

    @foreach (var item in @Model)
    {
        <div>
            <p>@item.Title</p>
            <br />
            <img height="100" src="@item.URL" />
        </div>
    }
</body>
</html>
